options {
  directory "/var/named" ;
  auth-nxdomain yes ;
  version "unknown" ;
  allow-query {
    any ;
  };
  allow-transfer {
    none ;
  };
  allow-recursion {
    any ;
  };
};
controls {
  inet 127.0.0.1 allow {
    localhost ;
  }
  keys {
    rndckey ;
  };
};
logging {
  channel "named_all.log" {
    syslog local3 ;
    severity debug ;
    print-time yes ;
    print-severity yes ;
    print-category yes ;
  };
  channel "named_queries.log" {
    syslog local4 ;
    severity info ;
    print-time yes ;
    print-severity yes ;
    print-category yes ;
  };
  category default {
    "named_all.log" ;
  };
  category queries {
    "named_queries.log" ;
  };
  category lame-servers {
    null ;
  };
};
include "martian.conf" ;
include "/etc/rndc.key" ;
zone "." IN {
  type hint ;
  file "named.ca" ;
};
zone "localhost" IN {
  type master ;
  file "localhost.zone" ;
  allow-update {
    none ;
  };
};
zone "0.0.127.in-addr.arpa" IN {
  type master ;
  file "named.local" ;
  allow-update {
    none ;
  };
};
